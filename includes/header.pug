header.header.d-flex.align-items-center
    .container
        .row.header-all
            .col-6.col-sm-4.col-md-2.header-all__logo.d-flex.align-items-center.justify-content-center
                img(src="img/logo.png")
            .col-8.header-all__nav
                ul.d-flex.align-items-center.justify-content-center
                    li
                        a(href="#") HOME
                    li
                        a(href="#") OUR PRODUCTS
                    li
                        a(href="#") BLOG
                    li
                        a(href="#") ABOUT
                    li
                        a(href="#") CONTACT
                    li
                        a(href="#") STYLEGUIDE
            .col-6.col-sm-5.col-md-2.header-all__cart.d-flex.justify-content-center
                .cart-wrapper.d-flex.align-items-center.justify-content-between
                    .cart-svg
                        svg(width='16' height='20' viewBox='0 0 16 20' fill='none' xmlns='http://www.w3.org/2000/svg' xmlns:xlink='http://www.w3.org/1999/xlink')
                            path(d='M0 0H16V20H0V0Z' fill='url(#pattern0)')
                            defs
                                pattern#pattern0(patternContentUnits='objectBoundingBox' width='1' height='1')
                                    use(xlink:href='#image0_10_34' transform='scale(0.03125 0.025)')
                                image#image0_10_34(width='32' height='40' xlink:href='data:image/png;base64,')
                    .cart-text CART
                    .cart-order 0
                .menu-wrapper
                    .hamburger-menu
